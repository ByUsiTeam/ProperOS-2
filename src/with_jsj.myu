//滑动窗体选项卡(option=选项,api=标识)
fn top_tab(option,api)
f(api=="首页选项卡")
{
	//窗体滑动第一个界面执行的操作,option从0开始
	f(option==0)
	{
		syso("当前滑动窗体是：实用软件")
	}
	f(option==1)
	{
		syso("当前滑动窗体是：绿色软件")
	}
	f(option==2)
	{
		syso("当前滑动窗体是：技术分享")
	}
	f(option==3)
	{
		syso("当前滑动窗体是：精品")
	}
	f(option==4)
	{
		syso("当前滑动窗体是：英雄联盟")
	}
	f(option==5)
	{
		syso("当前滑动窗体是：WithUI")
	}
}
end fn


