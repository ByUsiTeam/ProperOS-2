//top导航栏(code=控件n,api=标识)
fn V_top(code,api)
syso(api)
f(code==1)
{
	//图片控件1点击事件
	syso("图片控件1点击事件")
}
f(code==2)
{
	//图片控件2点击事件
	syso("图片控件2点击事件")
}
end fn


//单按钮弹窗-在线程中需要用上ufnsui,api=标识
fn A_oneUtw(api,fid,yid,uycls)
syso(api)

//关闭弹窗
fn with_ui.N_ender(1,fid,yid,uycls)
syso("按钮点击事件")
end fn


//双按钮弹窗-在线程中需要用上ufnsui,api=标识
fn A_twoUtw(api,fid,yid,uycls,code)
syso(api)

f(code==1)
{
	//按钮1点击事件
	syso("按钮1点击事件")
}
f(code==2)
{
	//按钮2点击事件
	syso("按钮2点击事件")
}

//关闭弹窗
fn with_ui.N_ender(2,fid,yid,uycls)
end fn


