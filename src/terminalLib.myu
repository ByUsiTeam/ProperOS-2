fn echo(text)

time(0,time)
ss("ProperOS_Two@CodeML:\n["+time+"]$ ~/",head)
ula(sss.terminal,3=head,4=text)
uls(6,sss.terminal,"terminal_list.iyu",-1-2)
gvs(6,list)
call(null,"mjava","output.awayroll",list)

end fn

fn echo(text,clear)

ula(sss.terminal,null)
time(0,time)
ss("ProperOS_Two@CodeML:\n["+time+"]$ ~/",head)
ula(sss.terminal,3=head,4=text)
uls(6,sss.terminal,"terminal_list.iyu",-1-2)
gvs(6,list)
call(null,"mjava","output.awayroll",list)

end fn